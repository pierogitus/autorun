{
    "name": "runwhiletyping",
    "displayName": "Run While Typing",
    "description": "Run While Typing",
    "version": "0.0.1",
    "publisher": "Alex Thompson",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.runWhileTyping"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.runWhileTyping",
            "title": "Run While Typing"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}